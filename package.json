{
  "name": "@indigo-labs/indigo-sdk",
  "version": "0.1.1",
  "description": "Indigo SDK for interacting with Indigo endpoints via lucid-evolution",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --minify --format esm,cjs --dts --clean",
    "lint": "eslint",
    "repack": "pnpm run build  && pnpm pack",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write src",
    "ts-node": "ts-node",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IndigoProtocol/indigo-sdk.git"
  },
  "keywords": [
    "indigo",
    "sdk",
    "typescript",
    "lucid-evolution",
    "cdp"
  ],
  "author": "3rd Eye Labs",
  "license": "MIT",
  "devDependencies": {
    "@sinclair/typebox": "^0.32.15",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "prettier": "^3.5.3",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@lucid-evolution/lucid": "^0.4.27",
    "dotenv": "^16.5.0"
  },
  "packageManager": "pnpm@9.0.4+sha1.b198ac6d38244fd829253720f9daafd6a606834d"
}